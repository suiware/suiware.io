---
import { SITE } from "@/config"


const structuredData = {
  "@context": "https://schema.org",
  "@type": "WebSite",
  name: SITE.title,
  url: SITE.website,
}
---

<script is:inline type="application/ld+json" set:html={JSON.stringify(structuredData)} />
<meta property="og:type" content="website" />
